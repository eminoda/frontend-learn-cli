const Router = require('express').Router()
const { API_CODE } = require('../lib/enums')
const logger = require('./lib/logger')('router:<%= filename %>')

Router.get('/', (req, res, next) => {
  try {
    return res.json({ code: API_CODE.SUCCESS, msg: '成功' })
  } catch (err) {
    logger.error(err)
    return res.json({ code: API_CODE.ERROR, msg: err.message })
  }
})

module.exports = Router


